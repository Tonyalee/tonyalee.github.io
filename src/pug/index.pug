	.content.content__profile
		.content__profile__section.content__profile__sec1
			.summery-box.summery-box--left
				.summery-box__photo
					img(src='/images/photo.jpg')
				.summery-box__descrip
					.summery-box__descrip__title Senior Front-End Dynamic Designer
					.summery-box__descrip__tool
			.summery-box.summery-box--right
				.summery-box__title
					h2 Hi, I'm
					h1 Tonya Lee
					h3 Designer x Developer
					p Strong passion to make a cross to front-end developing. High awareness and empathy to maximize user benefits in design practice. Blending designers’DNA into my daily exposures to languages including HTML, CSS and JS.

		.content__profile__section.content__profile__sec2
			.timeline-box
				.timeline-box__title.title
					h4.split Skill &amp; Experience
				.timeline-box__content
					ul.timeline-box__list
						each item, index in tlList
							li(class='stagger-item tl-list tl-list-' + item.id)
								.tl-list__deco
								.tl-list__wrap.list
									.list__headline #{item.headline}
									.list__companyName #{item.companyName}
									.list__date #{item.date}
									.list__type #{item.type}
									.list__content
										each point, i in item.content
											.list__content__point #{point}


		.content__profile__section.content__profile__sec3.highlight-bg
			.rewards-box
				.rewards-box__title
					h4 Honors &amp; Awards
				.rewards-box__content
					ul.rewards-box__list
						each item, index in rwList
							li(class='stagger-item rw-list rw-list-' + item.id)
								.rw-list__pic
									each itemImg, indexImg in item.imgUrl
										.rw-list__pic__img
											img(src=itemImg)
								.rw-list__wrap.list
									each itemheadline in item.headline
										.list__headline #{itemheadline}
									i - - - - - - - - - - - - - - - - - - - -   
									.list__companyName #{item.companyName}
									.list__date #{item.date}
									.list__type #{item.type} <br>
									i - - - - - - - - - - - - - - - - - - - -   
									.list__content
										each point, i in item.content
											.list__content__point #{point}